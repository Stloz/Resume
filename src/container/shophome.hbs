<main class="p-5">
    <hr>
    <div class="col-12">

        <div class="nav">
            {{#with navigation}}
            {{#each links}}
            <div class="nav-item">
                <a href="{{href}}" class="nav-link active" target="_blank">{{text}}</a>
            </div>
            {{/each}}
            {{/with}}
            <div class="nav-item">
                <a href="{{href}}" class="nav-link disabled" target="_blank">Disabled</a>
            </div>
        </div>
    </div>
    <hr>

</main>